{
  "name": "mb-eslint-config",
  "version": "1.0.1",
  "description": "Eslint 的规则配置",
  "keywords": [
    "eslint",
    "eslint-config",
    "conf"
  ],
  "license": "ISC",
  "author": "LiYong",
  "type": "module",
  "scripts": {
    "clear": "rm -fr node_modules",
    "lint": "pnpm eslint .",
    "test": "eslint \"./test/**/*.{css,tsx,vue,ts,js,html}\" --fix"
  },
  "main": "./src/index.js",
  "module": "./index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "default": "./index.mjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-command": "^2.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "jsonc-eslint-parser": "^2.4.0"
  }
